<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="./EJS.css">
    <title>Table</title>
</head>

<body>
    <table>
        <thead>
            <tr>
                <% header.forEach(e=>{%>
                <th> <%= e %></th>
                <%})%>
            </tr>

        </thead>
        <tbody>
            <% rows.forEach(row=>{%>
            <tr> <%row.forEach(e=>{ %>
                <td>
                    <%= e %>
                </td>
                <% })%>
            </tr>
            <% }) %>
        </tbody>
    </table>
</body>

</html>